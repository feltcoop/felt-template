{
  "name": "felt-template",
  "description": "a static website template with SvelteKit, Gro, and Felt",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">= 14.16.0"
  },
  "scripts": {
    "dev": "gro dev",
    "build": "gro build",
    "test": "gro test",
    "deploy": "gro deploy"
  },
  "devDependencies": {
    "@feltcoop/felt": "^0.12.0",
    "@feltcoop/gro": "^0.34.3",
    "@sveltejs/adapter-static": "^1.0.0-next.20",
    "@sveltejs/kit": "^1.0.0-next.179",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.43.1",
    "svelte-preprocess-esbuild": "^2.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "prettier": {
    "useTabs": true,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "svelteBracketNewLine": true,
    "overrides": [
      {
        "files": "package.json",
        "options": {
          "useTabs": false
        }
      }
    ]
  }
}
